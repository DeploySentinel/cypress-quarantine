{
  "name": "@deploysentinel/cypress-runner",
  "license": "UNLICENSED",
  "version": "3.2.0",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "DeploySentinel"
  },
  "main": "./dist",
  "typings": "./dist",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run tsc && npm run swc",
    "dev": "run-p watch:*",
    "test": "jest",
    "tsc": "tsc",
    "swc": "swc src -d dist",
    "watch:tsc": "run-s \"tsc --watch --preserveWatchOutput\"",
    "watch:swc": "run-s \"swc --watch\""
  },
  "dependencies": {
    "acorn": "^8.8.0",
    "debug": "^4.3.2",
    "escodegen": "^2.0.0",
    "estraverse": "^5.3.0",
    "http-proxy": "^1.18.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "npm-which": "^3.0.1",
    "semver": "^7.3.8",
    "slash": "3.0.0",
    "source-map-support": "^0.5.21",
    "tmp": "^0.2.1"
  },
  "files": [
    "bin/*",
    "dist/*"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.49",
    "@swc/core": "^1.2.85",
    "@swc/jest": "^0.2.3",
    "@types/debug": "^4.1.7",
    "@types/estree": "^1.0.0",
    "@types/js-yaml": "^4.0.3",
    "@types/npm-which": "^3.0.1",
    "chokidar": "^3.5.2",
    "nanoid": "^3.1.25",
    "shelljs": "^0.8.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testMatch": [
      "**/*.spec.ts"
    ]
  }
}
